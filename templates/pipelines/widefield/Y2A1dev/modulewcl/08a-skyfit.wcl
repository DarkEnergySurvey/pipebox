<module>
    <skyfit>
        wrappername = generic.py
        wrapperloop = expnum,band
        modnamepat = ${modnamepat_expband}
        <file>
            <bleedmask_binned_fp>
                # what metadata to get
                filetype = bleedmask_binned_fp

                # how to name the file
                filepat = exposure
                flabel = bleedmask-binned-fp
                fsuffix = fits

                # where to put the file
                dirpat = se
                rundir = binned-fp
            </bleedmask_binned_fp>
            <skyfit_binned_fp>
                # what metadata to get
                filetype = skyfit_binned_fp

                # how to name the file
                filepat = exposure
                flabel = skyfit-binned-fp
                fsuffix = fits

                # where to put the file
                dirpat = se
                ops_enddir = binned-fp
                rundir = binned-fp

                # whether to bring file home
                savefiles = True
            </skyfit_binned_fp>
            <skypca_binned_fp>
                filetype = skypca_binned_fp

                # how to name the file
                filename = ${skycampaign}_${epochrange}_${band}_${skypcarun}_skypca-binned-fp.fits

                # where to put the file
                dirpat = se
                ops_enddir = binned-fp
                rundir = binned-fp
            </skypca_binned_fp>
        </file>
        <exec_1>
            execname = sky_fit
            cmd_hyphen = alldouble
            used = file.bleedmask_binned_fp,file.skypca_binned_fp
            was_generated_by = file.skyfit_binned_fp
            ancestry = file.bleedmask_binned_fp:file.skyfit_binned_fp
            <cmdline>
                infilename = ${file.bleedmask_binned_fp.fullname}
                pcfilename = ${file.skypca_binned_fp.fullname}
                outfilename = ${file.skyfit_binned_fp.fullname} 
            </cmdline>
        </exec_1>
    </skyfit>
</module>
