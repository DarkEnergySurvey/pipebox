<module>
    <mkbleedmask>
        wrappername = generic.py
        wrapperloop = ccdnum
        modnamepat=${modnamepat_ccdnumband}

        <file>
            <red_wcs>
                filetype = red
                filepat = ccdnum_band
                flabel = wcs
                fsuffix = fits
                dirpat = se
                ops_enddir = red
                rundir = red
            </red_wcs>
            <cat_satstars>
                filetype = cat_satstars
                filepat = ccdnum_band
                flabel = satstars
                fsuffix = fits
                dirpat = se
		optional = true
                ops_enddir = aux
                rundir = aux
            </cat_satstars>
            <cat_trailbox>
                filetype = cat_trailbox
                filepat = ccdnum_band
                flabel = trailbox
                fsuffix = fits
                dirpat = se
        	optional = true
                ops_enddir = aux
                rundir = aux
            </cat_trailbox>
            <red_bleedmasked>
                filetype = red
                filepat = ccdnum_band
                flabel = bleedmasked    
                fsuffix = fits
                dirpat = se
                ops_enddir = red
                rundir = red
                savefiles = false
            </red_bleedmasked>
        </file>

        <exec_1>
            execname = mkbleedmask
            used = file.red_wcs
            was_generated_by = file.cat_satstars,file.cat_trailbox,file.red_bleedmasked
            ancestry = file.red_wcs:file.red_bleedmasked,file.red_wcs:file.cat_satstars,file.red_wcs:file.cat_trailbox
            <cmdline>
                _01_red_wcs = ${file.red_wcs.fullname}
                _02_red_bleedmasked = ${file.red_bleedmasked.fullname}
		i = _flag
		m = _flag
		b = 5
		f = 1.0
		l = 7
		n = 7
		r = 5
                s = 100
                t = 20
		w = 2
		y = 1.0
                v = 3
		E = 10
		L = 30
                o = ${file.cat_satstars.fullname}
                x = ${file.cat_trailbox.fullname}
            </cmdline>
        </exec_1>
    </mkbleedmask>
</module>
